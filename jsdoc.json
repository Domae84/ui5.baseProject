{
	"tags": {
		"allowUnknownTags": false
	},
	"source": {
		"include": ["webapp/"],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false
	},
	"opts": {
		"template": "jsdoc/docdash",
		"encoding": "utf8",
		"lenient": false,
		"sort": false,
		"recurse": true,
		"readme": "README.md"
	},
	"docdash": {
		"sort": false
	}
}
