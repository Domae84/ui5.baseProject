{
	"_version": "1.12.0",
	"sap.app": {
		"id": "namespace",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"mainService": {
				"uri": "path/to/ODataService",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			},
			"util": {
				"uri": "model/json/Util.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"fullWidth": true
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "namespace.view.App",
			"type": "XML",
			"async": true,
			"id": "idAppControl"
		},
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ui.comp": {},
				"sap.uxap": {},
				"sap.ui.unified": {},
				"sap.ui.table": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"": {
				"dataSource": "mainService",
				"settings": {
					"defaultBindingMode": "TwoWay"
				},
				"preload": true
			},
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "isu.metaprodukte.ppm.i18n.i18n"
				}
			},
			"util": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "util"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "namespace.view",
				"controlId": "idAppControl",
				"controlAggregation": "pages",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "app",
					"target": "app"
				}
			],
			"targets": {
				"app": {
					"viewName": "App",
					"viewLevel": "1",
					"transition": "fade"
				}
			}
		}
	}
}
